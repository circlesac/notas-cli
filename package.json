{
  "name": "notas",
  "version": "26.2.0",
  "private": true,
  "type": "module",
  "module": "src/index.ts",
  "dependencies": {
    "@notionhq/client": "^5.9.0",
    "citty": "^0.2.1"
  },
  "devDependencies": {
    "@circlesac/lint": "^26.2.5",
    "@circlesac/oneup": "^26.2.5",
    "@types/bun": "latest",
    "@vitest/coverage-v8": "^4.0.18",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "build": "bun build --compile --outfile=dist/notas src/index.ts",
    "dev": "bun run src/index.ts",
    "test": "vitest run",
    "lint": "npx @circlesac/lint --all",
    "type-check": "npx tsc --noEmit",
    "pre-merge": "bun run lint && bun run type-check && bun run test && bun run build",
    "deploy:worker": "cd worker && npx wrangler deploy"
  }
}
